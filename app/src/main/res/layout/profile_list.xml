                android:layout_marginEnd="8dp"
                android:layout_marginBottom="8dp"
                android:hint="8,000"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
